instance sokoban_inst {

    domain = sokoban;

    non-fluents = nf_sokoban;
    
    init-state {
		box(x3, y3);
		box(x3, y5);

		man(x2, y6);
    };

    max-nondef-actions = 1;
    horizon = 10;
    discount = 1.0;
}


