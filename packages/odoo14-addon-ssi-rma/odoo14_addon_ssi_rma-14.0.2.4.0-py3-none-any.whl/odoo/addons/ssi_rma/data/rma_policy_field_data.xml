<odoo>
<data>
<record id="rma_policy_field_ordered" model="rma_policy_field">
    <field name="name">Ordered</field>
    <field name="code">uom_quantity</field>
    <field name="active" eval="1" />
</record>
<record id="rma_policy_field_received" model="rma_policy_field">
    <field name="name">Received</field>
    <field name="code">qty_received</field>
    <field name="active" eval="1" />
</record>
<record id="rma_policy_field_delivered" model="rma_policy_field">
    <field name="name">Delivered</field>
    <field name="code">qty_delivered</field>
    <field name="active" eval="1" />
</record>
<record id="rma_policy_field_rma_supplier" model="rma_policy_field">
    <field name="name">In RMA Supplier</field>
    <field name="code">qty_in_supplier_rma</field>
    <field name="active" eval="1" />
</record>
</data>
</odoo>
