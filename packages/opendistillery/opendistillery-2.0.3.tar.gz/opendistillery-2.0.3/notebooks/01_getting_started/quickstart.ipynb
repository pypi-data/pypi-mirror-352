    "# Sample business analysis task\n",
    "def submit_task(task_request):\n",
    "    response = requests.post(\n",
    "        f\"{API_BASE}/tasks\",\n",
    "        headers=headers,\n",
    "        json=task_request\n",
    "    )\n",
    "    \n",
    "    if response.status_code == 200:\n",
    "        return response.json()\n",
    "    else:\n",
    "        print(f\"âŒ Task submission failed: {response.text}\")\n",
    "        return None\n",
    "\n",
    "# Example 1: Market Analysis Task\n",
    "market_analysis_task = {\n",
    "    \"task_type\": \"market_analysis\",\n",
    "    \"description\": \"Analyze Q4 2024 market trends for technology sector\",\n",
    "    \"input_data\": {\n",
    "        \"sector\": \"technology\",\n",
    "        \"time_period\": \"Q4_2024\",\n",
    "        \"metrics\": [\"revenue_growth\", \"market_share\", \"competitive_landscape\"],\n",
    "        \"companies\": [\"Apple\", \"Microsoft\", \"Google\", \"Amazon\", \"Meta\"]\n",
    "    },\n",
    "    \"priority\": \"high\",\n",
    "    \"required_capabilities\": [\"analysis\", \"prediction\"],\n",
    "    \"context\": {\n",
    "        \"business_goal\": \"investment_decision\",\n",
    "        \"stakeholder\": \"investment_committee\",\n",
    "        \"deadline\": \"2024-12-31\"\n",
    "    }\n",
    "}\n",
    "\n",
    "# Example 2: Customer Segmentation Task\n",
    "customer_segmentation_task = {\n",
    "    \"task_type\": \"customer_segmentation\",\n",
    "    \"description\": \"Identify high-value customer segments for retention campaign\",\n",
    "    \"input_data\": {\n",
    "        \"customer_data\": {\n",
    "            \"total_customers\": 50000,\n",
    "            \"avg_customer_value\": 1250,\n",
    "            \"churn_rate\": 0.15,\n",
    "            \"segments\": [\"enterprise\", \"mid_market\", \"small_business\"]\n",
    "        },\n",
    "        \"analysis_goals\": [\n",
    "            \"identify_high_value_segments\",\n",
    "            \"predict_churn_risk\",\n",
    "            \"recommend_retention_strategies\"\n",
    "        ]\n",
    "    },\n",
    "    \"priority\": \"medium\",\n",
    "    \"required_capabilities\": [\"analysis\", \"research\"]\n",
    "}\n",
    "\n",
    "# Submit tasks\n",
    "print(\"ğŸš€ Submitting Tasks to OpenDistillery...\\n\")\n",
    "\n",
    "print(\"ğŸ“Š Task 1: Market Analysis\")\n",
    "task1_result = submit_task(market_analysis_task)\n",
    "if task1_result:\n",
    "    print(f\"   âœ… Task ID: {task1_result['task_id']}\")\n",
    "    print(f\"   ğŸ“Š Status: {task1_result['status']}\")\n",
    "    print(f\"   â±ï¸  Processing Time: {task1_result['processing_time']:.3f}s\")\n",
    "\n",
    "print(\"\\nğŸ‘¥ Task 2: Customer Segmentation\")\n",
    "task2_result = submit_task(customer_segmentation_task)\n",
    "if task2_result:\n",
    "    print(f\"   âœ… Task ID: {task2_result['task_id']}\")\n",
    "    print(f\"   ğŸ“Š Status: {task2_result['status']}\")\n",
    "    print(f\"   â±ï¸  Processing Time: {task2_result['processing_time']:.3f}s\")\n",
    "\n",
    "# Store task IDs for monitoring\n",
    "submitted_tasks = []\n",
    "if task1_result:\n",
    "    submitted_tasks.append(task1_result['task_id'])\n",
    "if task2_result:\n",
    "    submitted_tasks.append(task2_result['task_id'])\n",
    "\n",
    "print(f\"\\nğŸ“‹ Total Tasks Submitted: {len(submitted_tasks)}\")"
   ]
