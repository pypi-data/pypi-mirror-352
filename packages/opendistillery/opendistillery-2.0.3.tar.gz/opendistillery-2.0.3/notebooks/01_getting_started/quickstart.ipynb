  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# OpenDistillery API Configuration\n",
    "API_BASE = \"http://localhost:8000\"\n",
    "API_KEY = \"opd_demo_enterprise_key_12345\"  # Replace with your actual API key\n",
    "\n",
    "# Headers for API requests\n",
    "headers = {\n",
    "    \"Authorization\": f\"Bearer {API_KEY}\",\n",
    "    \"Content-Type\": \"application/json\"\n",
    "}\n",
    "\n",
    "# Test API connection\n",
    "def test_connection():\n",
    "    try:\n",
    "        response = requests.get(f\"{API_BASE}/health\", headers=headers)\n",
    "        if response.status_code == 200:\n",
    "            health_data = response.json()\n",
    "            print(\"âœ… API Connection Successful!\")\n",
    "            print(f\"Status: {health_data['status']}\")\n",
    "            print(f\"Version: {health_data['version']}\")\n",
    "            print(f\"Active Agents: {health_data['metrics']['active_agents']}\")\n",
    "            return True\n",
    "        else:\n",
    "            print(f\"âŒ API Error: {response.status_code}\")\n",
    "            return False\n",
    "    except Exception as e:\n",
    "        print(f\"âŒ Connection Failed: {str(e)}\")\n",
    "        return False\n",
    "\n",
    "test_connection()"
   ]
