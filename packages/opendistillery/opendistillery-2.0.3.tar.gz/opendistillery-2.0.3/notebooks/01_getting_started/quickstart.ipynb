    "        print(f\"   Capabilities: {', '.join(agent['capabilities'])}\")\n",
    "        print(f\"   State: {agent['state']}\")\n",
    "        print(f\"   Current Load: {agent['current_load']:.1%}\")\n",
    "        print(f\"   Success Rate: {agent['success_rate']:.1%}\")\n",
