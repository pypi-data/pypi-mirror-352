    "# Demonstrate batch task submission\n",
    "print(\"üöÄ Advanced Feature: Batch Task Processing\\n\")\n",
    "\n",
    "# Create multiple related tasks\n",
    "batch_tasks = [\n",
    "    {\n",
    "        \"task_type\": \"sentiment_analysis\",\n",
    "        \"description\": \"Analyze customer feedback sentiment\",\n",
    "        \"input_data\": {\n",
    "            \"feedback_source\": \"social_media\",\n",
    "            \"sample_size\": 1000,\n",
    "            \"channels\": [\"twitter\", \"facebook\", \"instagram\"]\n",
    "        },\n",
    "        \"priority\": \"medium\",\n",
    "        \"required_capabilities\": [\"analysis\"]\n",
    "    },\n",
    "    {\n",
    "        \"task_type\": \"competitive_analysis\",\n",
    "        \"description\": \"Analyze competitor pricing strategies\",\n",
    "        \"input_data\": {\n",
    "            \"competitors\": [\"CompanyA\", \"CompanyB\", \"CompanyC\"],\n",
    "            \"product_categories\": [\"software\", \"services\", \"support\"],\n",
    "            \"analysis_period\": \"6_months\"\n",
    "        },\n",
    "        \"priority\": \"high\",\n",
    "        \"required_capabilities\": [\"analysis\", \"research\"]\n",
    "    },\n",
    "    {\n",
    "        \"task_type\": \"trend_forecasting\",\n",
    "        \"description\": \"Forecast technology trends for 2025\",\n",
    "        \"input_data\": {\n",
    "            \"focus_areas\": [\"AI\", \"Cloud\", \"Cybersecurity\", \"IoT\"],\n",
    "            \"forecast_horizon\": \"12_months\",\n",
    "            \"confidence_level\": 0.85\n",
    "        },\n",
    "        \"priority\": \"high\",\n",
    "        \"required_capabilities\": [\"prediction\", \"analysis\"]\n",
    "    }\n",
    "]\n",
    "\n",
    "# Submit batch tasks\n",
    "batch_response = requests.post(\n",
    "    f\"{API_BASE}/tasks/batch\",\n",
    "    headers=headers,\n",
    "    json=batch_tasks\n",
    ")\n",
    "\n",
    "if batch_response.status_code == 200:\n",
