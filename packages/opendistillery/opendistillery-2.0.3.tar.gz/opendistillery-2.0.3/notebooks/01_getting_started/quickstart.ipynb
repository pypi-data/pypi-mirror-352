    "## Step 3: Register Custom Agents (If Needed)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Register a custom business analyst agent\n",
    "def register_agent(agent_config):\n",
    "    response = requests.post(\n",
    "        f\"{API_BASE}/agents\",\n",
    "        headers=headers,\n",
    "        json=agent_config\n",
    "    )\n",
    "    \n",
    "    if response.status_code == 200:\n",
    "        result = response.json()\n",
    "        print(f\"âœ… Agent '{agent_config['agent_id']}' registered successfully!\")\n",
    "        return True\n",
    "    else:\n",
    "        print(f\"âŒ Failed to register agent: {response.text}\")\n",
    "        return False\n",
    "\n",
    "# Example: Business Intelligence Agent\n",
    "business_agent_config = {\n",
    "    \"agent_id\": \"business_intelligence_analyst\",\n",
    "    \"agent_type\": \"analyst\",\n",
    "    \"capabilities\": [\"analysis\", \"prediction\", \"reporting\"],\n",
    "    \"specialization\": \"business_intelligence\",\n",
    "    \"configuration\": {\n",
    "        \"analysis_depth\": \"comprehensive\",\n",
    "        \"reporting_format\": \"executive_summary\",\n",
    "        \"confidence_threshold\": 0.8\n",
    "    }\n",
    "}\n",
    "\n",
    "# Example: Customer Insights Agent\n",
    "customer_agent_config = {\n",
    "    \"agent_id\": \"customer_insights_specialist\",\n",
    "    \"agent_type\": \"specialist\",\n",
    "    \"capabilities\": [\"analysis\", \"research\", \"synthesis\"],\n",
    "    \"specialization\": \"customer_analytics\",\n",
    "    \"configuration\": {\n",
    "        \"segment_analysis\": True,\n",
    "        \"churn_prediction\": True,\n",
    "        \"lifetime_value_calculation\": True\n",
    "    }\n",
    "}\n",
    "\n",
    "# Register agents\n",
    "print(\"ğŸš€ Registering Custom Agents...\\n\")\n",
    "register_agent(business_agent_config)\n",
    "register_agent(customer_agent_config)\n",
    "\n",
    "# Refresh agent list\n",
    "time.sleep(2)\n",
    "response = requests.get(f\"{API_BASE}/agents\", headers=headers)\n",
    "updated_agents = response.json()\n",
    "print(f\"\\nğŸ“Š Updated Total Agents: {updated_agents['total_agents']}\")"
   ]
