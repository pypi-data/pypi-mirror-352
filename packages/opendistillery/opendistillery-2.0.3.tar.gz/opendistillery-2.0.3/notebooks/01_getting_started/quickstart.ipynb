   "source": [
    "# Get comprehensive system status\n",
    "def get_system_status():\n",
    "    response = requests.get(f\"{API_BASE}/status\", headers=headers)\n",
    "    if response.status_code == 200:\n",
    "        return response.json()\n",
    "    return None\n",
    "\n",
    "# Get performance analytics\n",
    "def get_performance_analytics():\n",
    "    response = requests.get(f\"{API_BASE}/analytics/performance\", headers=headers)\n",
    "    if response.status_code == 200:\n",
    "        return response.json()\n",
    "    return None\n",
    "\n",
    "print(\"ğŸ“Š System Performance Analysis\\n\")\n",
    "print(\"=\"*50)\n",
    "\n",
    "# Get system status\n",
    "status = get_system_status()\n",
    "if status:\n",
    "    print(f\"\\nğŸ–¥ï¸  System Information:\")\n",
    "    print(f\"   Version: {status['system_info']['version']}\")\n",
    "    print(f\"   Environment: {status['system_info']['environment']}\")\n",
    "    print(f\"   Uptime: {status['system_info']['uptime_seconds']:.0f} seconds\")\n",
    "    \n",
    "    print(f\"\\nğŸ¤– Orchestration Status:\")\n",
    "    orch = status['orchestration']\n",
    "    print(f\"   Registered Agents: {orch['registered_agents']}\")\n",
    "    print(f\"   Queued Tasks: {orch['queued_tasks']}\")\n",
    "    print(f\"   Active Tasks: {orch['active_tasks']}\")\n",
    "    print(f\"   Completed Tasks: {orch['completed_tasks']}\")\n",
    "    \n",
    "    print(f\"\\nğŸ“ˆ Performance Metrics:\")\n",
    "    perf = status['performance']\n",
    "    print(f\"   Average Processing Time: {perf['avg_task_processing_time']:.3f}s\")\n",
    "    print(f\"   Success Rate: {perf['success_rate']:.1%}\")\n",
    "    print(f\"   Agent Utilization: {perf['agent_utilization']:.1%}\")\n",
    "\n",
    "# Get detailed analytics\n",
    "analytics = get_performance_analytics()\n",
    "if analytics:\n",
    "    print(f\"\\nğŸ“Š Detailed Analytics ({analytics['time_range']}):\")\n",
    "    \n",
    "    sys_metrics = analytics['system_metrics']\n",
    "    print(f\"\\n   System Metrics:\")\n",
    "    print(f\"   â€¢ Total Tasks Processed: {sys_metrics['total_tasks_processed']}\")\n",
    "    print(f\"   â€¢ Success Rate: {sys_metrics['success_rate']:.1%}\")\n",
    "    print(f\"   â€¢ Average Processing Time: {sys_metrics['average_processing_time']:.3f}s\")\n",
    "    print(f\"   â€¢ Agent Utilization: {sys_metrics['agent_utilization']:.1%}\")\n",
    "    \n",
    "    agent_perf = analytics['agent_performance']\n",
    "    print(f\"\\n   Agent Performance:\")\n",
    "    print(f\"   â€¢ Total Agents: {agent_perf['total_agents']}\")\n",
    "    print(f\"   â€¢ Active Agents: {agent_perf['active_agents']}\")\n",
    "    print(f\"   â€¢ Average Success Rate: {agent_perf['average_agent_success_rate']:.1%}\")\n",
    "\n",
    "# Create performance visualization\n",
    "if analytics:\n",
    "    fig, axes = plt.subplots(2, 2, figsize=(15, 10))\n",
    "    \n",
    "    # System metrics pie chart\n",
    "    ax1 = axes[0, 0]\n",
    "    success_count = sys_metrics['total_tasks_processed'] * sys_metrics['success_rate']\n",
    "    failed_count = sys_metrics['total_tasks_processed'] * (1 - sys_metrics['success_rate'])\n",
    "    ax1.pie([success_count, failed_count], labels=['Successful', 'Failed'], \n",
    "            autopct='%1.1f%%', colors=['#2ecc71', '#e74c3c'])\n",
    "    ax1.set_title('Task Success Distribution')\n",
    "    \n",
    "    # Agent utilization\n",
    "    ax2 = axes[0, 1]\n",
    "    utilization_data = [sys_metrics['agent_utilization'], 1 - sys_metrics['agent_utilization']]\n",
    "    ax2.pie(utilization_data, labels=['Utilized', 'Available'], \n",
    "            autopct='%1.1f%%', colors=['#3498db', '#95a5a6'])\n",
    "    ax2.set_title('Agent Utilization')\n",
    "    \n",
    "    # Performance timeline (simulated)\n",
    "    ax3 = axes[1, 0]\n",
    "    times = pd.date_range(start='2024-01-01', periods=24, freq='H')\n",
