    "## Step 5: Monitor Task Progress"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Monitor task progress with real-time updates\n",
    "def get_task_status(task_id):\n",
    "    response = requests.get(f\"{API_BASE}/tasks/{task_id}\", headers=headers)\n",
    "    if response.status_code == 200:\n",
    "        return response.json()\n",
    "    return None\n",
    "\n",
    "def monitor_tasks(task_ids, max_wait_time=60):\n",
    "    print(\"ğŸ” Monitoring Task Progress...\\n\")\n",
    "    \n",
    "    start_time = time.time()\n",
    "    completed_tasks = []\n",
    "    \n",
    "    while len(completed_tasks) < len(task_ids) and (time.time() - start_time) < max_wait_time:\n",
    "        for task_id in task_ids:\n",
    "            if task_id not in completed_tasks:\n",
    "                status = get_task_status(task_id)\n",
    "                if status:\n",
    "                    print(f\"ğŸ“‹ Task {task_id[:8]}...\")\n",
    "                    print(f\"   Status: {status['status']}\")\n",
    "                    \n",
    "                    if status['status'] == 'completed':\n",
    "                        print(f\"   âœ… Completed Successfully!\")\n",
    "                        print(f\"   ğŸ¯ Confidence: {status.get('confidence', 'N/A')}\")\n",
    "                        print(f\"   ğŸ¤– Agent: {status.get('agent_id', 'N/A')}\")\n",
    "                        print(f\"   â±ï¸  Total Time: {status['processing_time']:.3f}s\")\n",
    "                        \n",
    "                        # Display key results\n",
    "                        if status.get('result'):\n",
    "                            result = status['result']\n",
    "                            print(f\"   ğŸ“Š Result Preview:\")\n",
    "                            for key, value in list(result.items())[:3]:  # Show first 3 items\n",
    "                                if isinstance(value, (str, int, float)):\n",
    "                                    print(f\"      {key}: {value}\")\n",
    "                        \n",
    "                        completed_tasks.append(task_id)\n",
    "                        print(\"   \" + \"-\"*40)\n",
    "                    \n",
    "                    elif status['status'] == 'failed':\n",
    "                        print(f\"   âŒ Failed: {status.get('error', 'Unknown error')}\")\n",
    "                        completed_tasks.append(task_id)\n",
    "                        print(\"   \" + \"-\"*40)\n",
    "                    \n",
    "                    else:\n",
    "                        print(f\"   â³ Status: {status['status']}...\")\n",
    "        \n",
    "        if len(completed_tasks) < len(task_ids):\n",
    "            time.sleep(2)  # Wait 2 seconds before next check\n",
    "    \n",
    "    print(f\"\\nâœ… Monitoring Complete: {len(completed_tasks)}/{len(task_ids)} tasks finished\")\n",
    "    return completed_tasks\n",
    "\n",
    "# Monitor our submitted tasks\n",
    "if submitted_tasks:\n",
    "    completed = monitor_tasks(submitted_tasks)\n",
    "else:\n",
    "    print(\"âš ï¸ No tasks to monitor\")"
   ]
