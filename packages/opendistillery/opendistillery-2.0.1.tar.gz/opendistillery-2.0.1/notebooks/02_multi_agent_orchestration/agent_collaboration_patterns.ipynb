    "            \"specialists\": [],\n",
    "            \"validators\": [],\n",
    "            \"executors\": []\n",
    "        }\n",
    "        \n",
    "        for agent_id, agent in self.agents.items():\n",
    "            if agent.archetype == AgentArchetype.MANAGER:\n",
    "                hierarchy[\"managers\"].append(agent_id)\n",
    "            elif agent.archetype == AgentArchetype.COORDINATOR:\n",
    "                hierarchy[\"coordinators\"].append(agent_id)\n",
    "            elif agent.archetype == AgentArchetype.ANALYST:\n",
    "                hierarchy[\"analysts\"].append(agent_id)\n",
    "            elif agent.archetype == AgentArchetype.SPECIALIST:\n",
