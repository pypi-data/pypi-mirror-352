#!/bin/sh

find . -name *pyc  |xargs rm -rf 2>/dev/null
find . -name build |xargs rm -rf 2>/dev/null
find . -name lib   |xargs rm -rf 2>/dev/null
find . -name *ovf  |xargs rm -rf 2>/dev/null
find . -name *png  |xargs rm -rf 2>/dev/null
find . -name *gif  |xargs rm -rf 2>/dev/null
find . -name *dat  |xargs rm -rf 2>/dev/null
find . -name *txt  |xargs rm -rf 2>/dev/null
find . -name *.out |xargs rm -rf 2>/dev/null
find . -name *.log |xargs rm -rf 2>/dev/null
find . -name pack* |xargs rm -rf 2>/dev/null
find . -name snap* |xargs rm -rf 2>/dev/null
find . -name *old  |xargs rm -rf 2>/dev/null
find . -name *pickle |xargs rm -rf 2>/dev/null
find . -name output  |xargs rm -rf 2>/dev/null
find . -name Spin_dynamics  |xargs rm -rf 2>/dev/null
find . -name *results*  |xargs rm -rf 2>/dev/null
find . -name __pycache__  |xargs rm -rf
find . -name report |xargs rm -rf
find . -name Ensemble* |xargs rm -rf
rm -rf dist
rm -rf pyasd.egg-info
rm -rf .git
