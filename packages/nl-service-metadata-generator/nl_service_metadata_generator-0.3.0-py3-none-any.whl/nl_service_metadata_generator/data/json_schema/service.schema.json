        "EPSG:3034",
        "EPSG:2213",
        "EPSG:4258",
        "EPSG:4326"
      ],
      "description": "optional field, only required when generating SDS interoperable service metadata record"
    },
    "boundingBox": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "minX": {
          "type": "number"
        },
        "minY": {
          "type": "number"
        },
        "maxX": {
          "type": "number"
        },
        "maxY": {
          "type": "number"
        }
      },
      "required": ["minX", "minY", "maxX", "maxY"]
    },
    "linkedDatasets": {
      "type": "array",
      "minItems": 1,
      "uniqueItems": true,
      "items": {
        "type": "object",
        "properties": {
          "datasetMdIdentifier": {
            "type": "string",
            "format": "uuid"
          }
        },
        "required": ["datasetMdIdentifier"]
      }
    },
    "service_license":{
      "type": "object",
      "minItems": 0,
      "properties":{
        "description": {
          "type": "string"
        },
        "url": {
          "type": "string",
          "format": "uri"
        }
      }
    },
    "thumbnails": {
      "type": "array",
      "minItems": 0,
      "uniqueItems": true,
      "items": {
        "type": "object",
        "properties": {
          "file": {
            "type": "string"
          },
          "description": {
            "type": "string"
          },
          "filetype": {
